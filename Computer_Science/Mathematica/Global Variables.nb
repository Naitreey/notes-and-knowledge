Notebook[{

Cell[CellGroupData[{
Cell["Global Expression Control", "Subtitle",
 CellChangeTimes->{{3.618387486730612*^9, 3.6183874993724904`*^9}}],

Cell[CellGroupData[{

Cell[TextData[Cell[BoxData[
 FormBox["$RecursionLimit", TraditionalForm]]]], "Section",
 CellChangeTimes->{{3.6183870012740817`*^9, 3.618387017658203*^9}}],

Cell[TextData[{
 "the current limit on the number of levels of recursion that ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " can use."
}], "ItemParagraph",
 CellChangeTimes->{3.618387560838541*^9}],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"$RecursionLimit", "=", "n"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " set the limit of recursion to be ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". If ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is set to be ",
 Cell[BoxData[
  FormBox["Infinity", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", recursion level is not limited at all."
}], "Text",
 CellChangeTimes->{{3.6183875935704117`*^9, 3.618387670045823*^9}}],

Cell[TextData[{
 "$RecursionLimit limits the maximum depth of the evaluation stack, or \
equivalently, the maximum nesting depth that would occur in the list \
structure produced by ",
 Cell[BoxData[
  FormBox["Trace", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ."
}], "Text",
 CellChangeTimes->{{3.618388908428748*^9, 3.618388929243782*^9}}],

Cell[TextData[{
 "The length of the list returned by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Stack", "[", "]"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " equals to the level of recursion used for evaluating an expression."
}], "Text",
 CellChangeTimes->{{3.618387726863493*^9, 3.618387783923134*^9}, {
  3.6183878200198107`*^9, 3.618387875821951*^9}, {3.6183879225205994`*^9, 
  3.618387956070828*^9}}],

Cell[TextData[{
 "When Mathematica stops without finishing evaluation, it returns a held \
result. You can continue the evaluation by explicitly calling ",
 Cell[BoxData[
  FormBox["ReleaseHold", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ."
}], "Text",
 CellChangeTimes->{{3.618388126827892*^9, 3.6183881445170097`*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[Cell[BoxData[
 FormBox["$IterationLimit", TraditionalForm]],
 FormatType->"TraditionalForm"]], "Section",
 CellChangeTimes->{{3.618388788377898*^9, 3.6183887923314385`*^9}}],

Cell["\<\
the maximum length of evaluation chain used in trying to evaluate any \
expression.\
\>", "ItemParagraph",
 CellChangeTimes->{3.618388809114069*^9}],

Cell[TextData[{
 Cell[BoxData["$IterationLimit"], "InlineFormula"],
 " gives an upper limit on the length of any list that can be generated by ",
 Cell[BoxData["Trace"], "InlineFormula"],
 ". "
}], "Text",
 CellChangeTimes->{{3.6183888525323095`*^9, 3.6183888616112833`*^9}}]
}, Open  ]]
}, Closed]],

Cell[CellGroupData[{

Cell["Namespace Management", "Subtitle",
 CellChangeTimes->{{3.6187457328437557`*^9, 3.618745738484913*^9}}],

Cell[CellGroupData[{

Cell[TextData[Cell[BoxData[
 FormBox["$ModuleNumber", TraditionalForm]],
 FormatType->"TraditionalForm"]], "Section",
 CellChangeTimes->{{3.6187457440009995`*^9, 3.618745747470003*^9}}],

Cell["\<\
gives the current serial number to be used for local variables that are \
created. \
\>", "ItemParagraph",
 CellChangeTimes->{3.618746149105166*^9}],

Cell[TextData[{
 Cell[BoxData[
  FormBox["$ModuleNumber", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is incremented every time ",
 Cell[BoxData[
  FormBox["Module", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " or ",
 Cell[BoxData[
  FormBox["Unique", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is called."
}], "Text",
 CellChangeTimes->{{3.618746198875538*^9, 3.6187462100871267`*^9}, {
  3.61874756803009*^9, 3.618747584062792*^9}}],

Cell[TextData[{
 "Every ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " session starts with ",
 Cell[BoxData[
  FormBox["$ModuleNumber", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " reset to 1."
}], "Text",
 CellChangeTimes->{{3.618747594829238*^9, 3.6187476140653877`*^9}}]
}, Open  ]]
}, Closed]],

Cell[CellGroupData[{

Cell["Mathemaitca Sessions", "Subtitle",
 CellChangeTimes->{{3.6187839438524694`*^9, 3.618783952212797*^9}}],

Cell[CellGroupData[{

Cell[TextData[Cell[BoxData[
 FormBox["$SessionID", TraditionalForm]],
 FormatType->"TraditionalForm"]], "Section",
 CellChangeTimes->{{3.6187839571348915`*^9, 3.618783960416602*^9}}],

Cell[TextData[{
 "a number set up to be unique to a particular ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " session. "
}], "ItemParagraph",
 CellChangeTimes->{3.6187839731519938`*^9}],

Cell[TextData[{
 Cell[BoxData[
  FormBox["$SessionID", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " should be different for different ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " sessions run either on the same computer or on different computers. "
}], "Text",
 CellChangeTimes->{{3.6187839917788067`*^9, 3.6187839982481966`*^9}}],

Cell[TextData[{
 "The value of ",
 Cell[BoxData["$SessionID"], "InlineFormula"],
 " is based on ",
 Cell[BoxData["$MachineID"], "InlineFormula"],
 ", as well as ",
 Cell[BoxData[
  RowBox[{"AbsoluteTime", "[", "]"}]], "InlineFormula"],
 " and operating system parameters such as the ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " process ID. "
}], "Text",
 CellChangeTimes->{{3.6187840236098413`*^9, 3.618784055987777*^9}}]
}, Open  ]]
}, Closed]]
